  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "debug with bst proxy",
      "runtimeExecutable": "npm",
      "env": { "SLS_DEBUG": "*" },
      "runtimeArgs": ["run", "debug"],
      "port": 9229,
      "cwd": "${workspaceFolder}/lambda/custom",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
